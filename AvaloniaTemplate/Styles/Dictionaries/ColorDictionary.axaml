<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Color x:Key="TransparentBrush">Transparent</Color>
	<Color x:Key="WhiteBrush">White</Color>
	<Color x:Key="BlackBrush">Black</Color>
	<Color x:Key="LightBlueBrush">#D4E7EE</Color>
	<Color x:Key="BlueBrush">#018ABB</Color>
	<Color x:Key="LightGrayBrush">#454545</Color>
	<Color x:Key="GrayBrush">#4D4D4D</Color>
	<Color x:Key="DarkGrayBrush">#484e57</Color>
	<LinearGradientBrush x:Key="GradientBack" StartPoint="0%, 0%" EndPoint="0%, 100%">
		<GradientStop Color="{StaticResource WhiteBrush}" Offset="0"/>
		<GradientStop Color="{StaticResource BlueBrush}" Offset="1"/>
	</LinearGradientBrush>

</ResourceDictionary>
